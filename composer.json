{
    "name": "hgajjar/core-package-composer-installer",
    "description": "A composer plugin for installing core composer package",
    "type": "composer-plugin",
    "license": "MIT",
    "require": {
        "php": "^5.5 | ^7.0",
        "composer-plugin-api": "~1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~1.5",
        "composer/composer": "^1.3@dev",
        "phpunit/phpunit": "~4.3",
        "mikey179/vfsStream": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "AydinHassan\\MagentoCoreComposerInstaller\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AydinHassan\\MagentoCoreComposerInstallerTest\\": "test/"
        }
    },
    "extra": {
        "class": "\\AydinHassan\\MagentoCoreComposerInstaller\\CoreManager"
    }
}
